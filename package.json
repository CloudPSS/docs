{
  "name": "docs",
  "version": "5.4.7",
  "homepage": "https://kb.cloudpss.net",
  "repository": {
    "type": "git",
    "url": "https://github.com/CloudPSS/docs"
  },
  "engines": {
    "node": ">=22.10"
  },
  "packageManager": "pnpm@9.15.2",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc",
    "pangu": "node ./tools/pangu.js",
    "optimize": "pnpm run convert-webp",
    "convert-webp": "node ./tools/convert-webp.js 'docs' 'blog'",
    "optimize-zip": "node ./tools/optimize-zip.js 'docs' 'blog'",
    "comp-docs": "node ./tools/comp-docs.js",
    "prepare": "husky && git config --local include.path ../.gitconfig || echo Failed to initialize husky",
    "format": "prettier --cache --cache-strategy content --write .",
    "lint": "eslint --cache --cache-location node_modules/.cache/eslint/ --cache-strategy content"
  },
  "prettier": "@cloudpss/prettier-config",
  "commitlint": {
    "extends": "@cloudpss"
  },
  "dependencies": {
    "@cloudpss/web-components": "^0.8.0",
    "@docusaurus/core": "3.7.0",
    "@docusaurus/plugin-pwa": "^3.7.0",
    "@docusaurus/preset-classic": "3.7.0",
    "@docusaurus/theme-common": "^3.7.0",
    "@docusaurus/theme-mermaid": "^3.7.0",
    "@easyops-cn/docusaurus-search-local": "^0.47.0",
    "@mdx-js/react": "^3.1.0",
    "clsx": "^2.1.1",
    "docusaurus-plugin-image-zoom": "^2.0.0",
    "hast-util-whitespace": "^3.0.0",
    "hastscript": "^9.0.0",
    "katex": "^0.16.21",
    "prism-react-renderer": "^2.4.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "rehype-katex": "^7.0.1",
    "remark-definition-list": "^2.0.0",
    "remark-directive": "^3.0.0",
    "remark-extended-table": "^2.0.2",
    "remark-frontmatter": "^5.0.0",
    "remark-gfm": "^4.0.0",
    "remark-ins": "^1.1.0",
    "remark-join-cjk-lines": "^1.0.13",
    "remark-math": "^6.0.0",
    "unist-util-remove": "^4.0.0",
    "unist-util-visit": "^5.0.0"
  },
  "devDependencies": {
    "@cloudpss/commitlint-config": "^3.0.18",
    "@cloudpss/eslint-config": "^3.0.18",
    "@cloudpss/http-client": "^0.5.43",
    "@cloudpss/lint-staged-config": "^3.0.18",
    "@cloudpss/prettier-config": "^3.0.18",
    "@cloudpss/resource-types": "^0.5.43",
    "@cloudpss/tsconfig": "^3.0.18",
    "@docusaurus/module-type-aliases": "3.7.0",
    "@docusaurus/tsconfig": "3.7.0",
    "@docusaurus/types": "3.7.0",
    "@types/download": "^8.0.5",
    "@types/hast": "^3.0.4",
    "@types/mdast": "^4.0.4",
    "@types/node": "^22.10.5",
    "@types/react": "^19.0.6",
    "chalk-template": "^1.1.0",
    "download": "^8.0.0",
    "execa": "^9.5.2",
    "glob": "^11.0.1",
    "gzipper": "^8.2.0",
    "husky": "^9.1.7",
    "image-size": "^1.2.0",
    "jszip": "^3.10.1",
    "postcss-preset-env": "^10.1.3",
    "pretty-bytes": "^6.1.1",
    "rxjs": "^7.8.1",
    "typescript": "~5.7.3"
  }
